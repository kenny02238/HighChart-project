{
    "extends":[
        "airbnb",
        "airbnb-typescript"
    ],
    "parserOptions": {
        "project": "./tsconfig.json",
        "extraFileExtensions": ["html"]
    },
    "rules": {
        "jsx-a11y/no-static-element-interactions": "off",
        "jsx-a11y/click-events-have-key-events": 0,
        "import/prefer-default-export": "off"
      }
}